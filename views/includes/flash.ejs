<%if(success.length){%>
<div class="alert alert-success col-6 offset-3 alert-dismissible" role="alert">
    <%=success%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<%}%>

<%if(error.length){%>
<div class="alert alert-danger col-6 offset-3 alert-dismissible" role="alert">
    <%=error%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<%}%>